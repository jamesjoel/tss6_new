<%- include('../includes/header') %>

<div class="container" style="min-height : 700px">
    <div class="row">
        
        <div class="col-md-12">
            <h2>List of All Student</h2>
            <table class="table table-bordered table-dark table-hover table-striped">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Contact</th>
                </tr>
                <% result.forEach((x, n)=>{ %>
                    <tr>
                        <td><%= n+1 %></td>
                        <td><%= x.name %></td>
                        <td><%= x.contact %></td>
                <% }) %>
            </table>

        </div>
        
    </div>
</div>
<%- include('../includes/footer') %>